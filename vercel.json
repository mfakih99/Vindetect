{
  "version": 2,
  "rewrites": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.vindetect.ai"
        }
      ],
      "destination": "/JsonToCSV/:path*"
    }
  ],
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.{js,css}",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/JsonToCSV",
      "dest": "/index.html"
    },
    {
      "src": "/JsonToCSV/styles.css",
      "dest": "/styles.css"
    },
    {
      "src": "/JsonToCSV/script.js",
      "dest": "/script.js"
    },
    {
      "src": "/JsonToCSV/(.*)",
      "dest": "/$1"
    }
  ],
  "trailingSlash": false,
  "cleanUrls": true
} 