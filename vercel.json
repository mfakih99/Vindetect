{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.{js,css}",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/JsonToCSV",
      "dest": "/index.html"
    },
    {
      "src": "/JsonToCSV/(.*)",
      "dest": "/$1"
    }
  ]
} 