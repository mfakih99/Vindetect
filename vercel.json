{
  "version": 2,
  "rewrites": [
    {
      "source": "/JsonToCSV",
      "destination": "/"
    },
    {
      "source": "/JsonToCSV/:match*",
      "destination": "/:match*"
    }
  ],
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.{js,css}",
      "use": "@vercel/static"
    }
  ],
  "trailingSlash": false,
  "cleanUrls": true
} 