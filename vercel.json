{
  "version": 2,
  "rewrites": [
    {
      "source": "/JsonToCSV",
      "destination": "/"
    },
    {
      "source": "/JsonToCSV/:match*",
      "destination": "/:match*"
    }
  ],
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.{js,css}",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/JsonToCSV",
      "dest": "/index.html"
    },
    {
      "src": "/JsonToCSV/styles.css",
      "dest": "/styles.css"
    },
    {
      "src": "/JsonToCSV/script.js",
      "dest": "/script.js"
    },
    {
      "src": "/JsonToCSV/(.*)",
      "dest": "/$1"
    }
  ],
  "trailingSlash": false,
  "cleanUrls": true
} 